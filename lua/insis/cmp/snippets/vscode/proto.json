{
  "proto syntax": {
    "prefix": "proto",
    "body": [
      "syntax = \"proto3\";",
      "",
      "package ${1:com.example};",
      "",
      "// 导入其他 proto 文件",
      "// import \"google/protobuf/empty.proto\";",
      "",
      "${0}"
    ],
    "description": "Proto3 基础模板"
  },
  "message definition": {
    "prefix": "msg",
    "body": [
      "message ${1:MessageName} {",
      "\t${2:int32} ${3:field_name} = ${4:1};",
      "}"
    ],
    "description": "Proto 消息定义"
  },
  "service definition": {
    "prefix": "svc",
    "body": [
      "service ${1:ServiceName} {",
      "\trpc ${2:MethodName}(${3:Request}) returns (${4:Response});",
      "}"
    ],
    "description": "Proto 服务定义"
  },
  "enum definition": {
    "prefix": "enum",
    "body": [
      "enum ${1:EnumName} {",
      "\t${2:ENUM_VALUE} = ${3:0};",
      "}"
    ],
    "description": "Proto 枚举定义"
  },
  "comment todo": {
    "prefix": "todo",
    "body": "// TODO: ${1}"
  }
}
